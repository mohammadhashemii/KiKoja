<page  clearBackStack="true" background="" toolbarcolor="#b4d9c7" title="ثبت‌نام" statusbarcolor="#d8f2e5" scrollable="false">
	<frame scrollable="true"  layout="linear_vertical" margin_top="0" margin_bottom="0">
		<image width="100" src="https://raw.githubusercontent.com/mohammadhashemii/KiKoja/master/src/main/images/icon-kikoja.png"></image>
	</frame>
	<frame layout="linear_vertical">
		<frame layout="linear_vertical"  margin_left="15" margin_right="15" >
			<text margin_left="10" margin_right="10" background="white">نام:</text>
			<textInput textsize="11" textalignment="right" margin-right="50" hint="علی" id="firstNameRegisterInput" onClick="firstNameRegisterInputClick"></textInput>
		</frame>
		<frame layout="linear_vertical"  margin_left="15" margin_right="15" >
			<text margin_left="10" margin_right="10" background="white">نام خانوادگی:</text>
			<textInput textsize="11" textalignment="right" margin-right="50" hint="احمدی" id="lastNameRegisterInput" onClick="lastNameRegisterInputClick"></textInput>
		</frame>
	</frame>
	<frame  layout="linear_vertical" margin_top="120" >
		<button progressBarRadius="11" progressBarColor="white" borderCornerRadius="8" onClick="update/nextButtonOfFirstRegisterPage"  textcolor="black" textsize="10" background="#b4d9c7" height="35" >بعدی</button>
	</frame>
</page>